{
    "layer": "top",
    "position": "top",
    "reload_style_on_change": true,
    "modules-left": [
        "hyprland/workspaces",
        "privacy",
        "custom/pacman",
        "group/tray"
    ],
    "modules-center": [
        "clock"
    ],
    "modules-right": [
        "mpris",
        "group/perf",
        "battery",
        "group/configs",
        "custom/control-center"
    ],
    "hyprland/workspaces": {
