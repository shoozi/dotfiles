    "mpris": {
        "format": "{status_icon} {title} - {artist}",
        "format-stopped": "{status_icon}",
        "artist-len": 15,
        "title-len": 20,
        "player-icons": {
            "firefox": "Ôâ©",
            "spotify": "ÔÜº"
        },
        "status-icons": {
            "playing": "ÔÖÑ",
            "paused": "Ôäã",
            "stopped": "Ôäç"
        },
        "ignored-players": [
            "firefox"
        ]
    },
    "group/perf": {
        "orientation": "inherit",
        "drawer": {
            "transition-duration": 500,
            "transition-left-to-right": false,
            "children-class": "perf-element"
        },
        "modules": [
            "custom/perf-icon",
            "temperature",
            "cpu",
            "memory"
        ]
    },
    "custom/perf-icon": {
        "tooltip": false,
        "format": "Ó¥Ø"
    },
    "group/configs": {
        "orientation": "inherit",
        "drawer": {
            "transition-duration": 500,
            "transition-left-to-right": false,
            "children-class": "misc-element",
            "click-to-reveal": true
        },
        "modules": [
            "custom/config-icon",
            "bluetooth",
            "pulseaudio#input",
            "pulseaudio#output",
            "network"
        ]
    },
    "custom/config-icon": {
        "tooltip": false,
        "format": "Óöê"
    },
    "custom/control-center": {
        "tooltip": false,
        "format": "ÔÉâ",
        "on-click": "swaync-client -t -sw",
        "escape": true
    },
    "clock": {
        "format": "{:L%A %d/%m - %I:%M %p}",
        "format-alt": "{:L%I:%M:%S %p}",
        "interval": 1,
        "tooltip-format": "<tt>{calendar}</tt>",
        "smooth-scrolling-threshold": 0.01,
        "calendar": {
            "mode": "year",
            "mode-mon-col": 3,
            "weeks-pos": "right",
            "on-scroll": 1,
            "format": {
                "months": "<span color='#ffead3'><b>{}</b></span>",
                "days": "<span color='#ecc6d9'><b>{}</b></span>",
                "weeks": "<span color='#99ffdd'><b>W{}</b></span>",
                "weekdays": "<span color='#ffcc66'><b>{}</b></span>",
                "today": "<span color='#ff6699'><b><u>{}</u></b></span>"
            }
        },
        "actions": {
            "on-click-right": "mode",
            "on-scroll-up": "shift_up",
            "on-scroll-down": "shift_down"
        }
    },
    "network": {
        "format-wifi": "Ôá´",
        "format-ethernet": "Û∞õ≥",
        "format-disconnected": "Ó¨Å",
        "tooltip-format-disconnected": "Not connected",
        "tooltip-format-wifi": "{essid} ({signalStrength}%) Ôá´",
        "tooltip-format-ethernet": "{ifname} üñß  ÔÉò {bandwidthUpBits} ÔÉó {bandwidthDownBits}",
        "on-click": "kitty --class floating -e nmtui"
    },
    "bluetooth": {
        "format-on": "Û∞ÇØ",
        "format-off": "BT-off",
        "format-disabled": "Û∞Ç≤",
        "format-no-controller": "",
        "format-connected-battery": "{device_battery_percentage}% Û∞ÇØ",
        "format-alt": "{device_alias} Û∞ÇØ",
        "tooltip-format": "{controller_alias}\t{controller_address}\n\n{num_connections} connected",
        "tooltip-format-connected": "{controller_alias}\t{controller_address}\n\n{num_connections} connected\n\n{device_enumerate}",
        "tooltip-format-enumerate-connected": "{device_alias}\n{device_address}",
        "tooltip-format-enumerate-connected-battery": "{device_alias}\n{device_address}\n{device_battery_percentage}%",
        "on-click-right": "blueman-manager"
    },
    "battery": {
        "interval": 30,
        "states": {
            "good": 95,
            "warning": 30,
            "critical": 20
        },
        "format": "{capacity}% {icon}",
        "format-charging": "{capacity}% Û∞ÇÑ",
        "format-plugged": "{capacity}% Û∞ÇÑ Ó¨≠",
        "format-alt": "{time} {icon}",
        "format-icons": [
            "Û∞Åª",
            "Û∞Åº",
            "Û∞Åæ",
            "Û∞ÇÄ",
            "Û∞ÇÇ",
            "Û∞Åπ"
        ]
    },
    "custom/pacman": {
        "format": "{} Û∞ö∞",
        "interval": 30,
        "exec": "checkupdates | wc -l",
        "exec-if": "exit 0",
        "on-click": "kitty sh -c 'yay -Syu; echo Done - Press enter to exit; read'; pkill -SIGRTMIN+8 waybar",
        "signal": 8,
        "tooltip": false
    },
    "custom/endpoint": {
        "format": " ",
        "tooltip": false
    },
    "custom/tray-icon": {
        "format": "ÔÉó",
        "tooltip": true,
        "tooltip-format": "Tray",
        "rotate": 90
    },
    "group/tray": {
        "orientation": "inherit",
        "drawer": {
            "transition-duration": 500,
            "transition-left-to-right": true,
            "children-class": "tray-element"
        },
        "modules": [
            "custom/tray-icon",
            "tray"
        ]
    },
    "cpu": {
        "format": "{usage}% Ôíº",
        "tooltip": true
    },
    "memory": {
        "format": "{percentage}% ÓøÖ",
        "tooltip": true
    },
    "temperature": {
        "critical-threshold": 80,
        "format": "{temperatureC}¬∞ Ôãâ",
        "hwmon-path": "/sys/devices/pci0000:00/0000:00:18.3/hwmon/hwmon1/temp1_input",
        "tooltip-format": "CPU: {temperatureC}¬∞C"
    },
    "tray": {
        "icon-size": 14,
        "spacing": 10
    },
    "privacy": {
        "icon-spacing": 10,
        "icon-size": 12,
        "transition-duration": 250,
        "modules": [
            {
                "type": "screenshare",
                "tooltip": true,
                "tooltip-icon-size": 24
            },
            {
                "type": "audio-in",
                "tooltip": true,
                "tooltip-icon-size": 24
            }
        ],
        "ignore-monitor": true,
        "ignore": [
            {
                "type": "audio-in",
                "name": "cava"
            },
            {
                "type": "screenshare",
                "name": "obs"
            }
        ]
    },
    "pulseaudio#input": {
        "scroll-step": 1,
        "format": "{format_source}",
        "format-source": "{volume}% ÔÑ∞",
        "format-source-muted": "ÔÑ±",
        "tooltip-format": "{format_source}",
        "on-click": "pactl set-source-mute @DEFAULT_SOURCE@ toggle",
        "on-scroll-up": "pactl set-source-volume @DEFAULT_SOURCE@ +5%",
        "on-scroll-down": "pactl set-source-volume @DEFAULT_SOURCE@ -5%"
    },
    "pulseaudio#output": {
        "scroll-step": 1,
        "format": "{volume}% {icon}",
        "format-bluetooth": "Û∞Ç∞",
        "format-bluetooth-muted": "{format_muted}",
        "format-muted": "Û∞ùü",
        "format-icons": {
            "headphone": "ÔÄ•",
            "headset": "Óπô",
            "phone": "ÔÇï",
            "default": [
                "ÔÄß",
                "ÔÄß",
                "ÔÄ®",
                "ÔÄ®"
            ]
        },
        "tooltip-format": "{desc}",
        "on-click": "pactl set-sink-mute @DEFAULT_SINK@ toggle",
        "on-click-right": "pavucontrol"
    }
}
