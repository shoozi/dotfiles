            "class<UnrealEditor> title<.+>": "<span font_size='10pt'>Û∞¶±</span>",
            "class<UnrealEditor> title<>": "",
            "class<io.github.achetagames.epic_asset_manager>": "ÔÜ≥",
            "code": "Û∞®û",
            "title<btop>": "Û∞äö",
            "KeePassXC": "ÔÄ£",
            "Rofi": "Û∞Æ´",
            "music": "ÔÄÅ",
            "nautilus": "ÔÅº",
            "Qalculate!": "Ôá¨",
            "com.stremio.stremio": "Û±úÖ",
            "steam": "ÔÜ∂",
            "discord": "Ôáø",
            "heroic": "ÔÜ≥",
            "hl2_linux": "ÔÑõ",
            "class<Minecraft Launcher": "ÔÜ≥",
            "class<(Minecraft\\s\\d\\.\\d+\\.\\d+)>": "ÔÜ≤",
